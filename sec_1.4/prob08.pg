## DESCRIPTION
## Integrating factor
## ENDDESCRIPTION

## Institution(Dartmouth)
## KEYWORDS('linear' 'differential equation')



DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "Dartmouthmacros.pl",
  "PGcourse.pl"
);
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 0;

$alpha = random(2,20 );

## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());


BEGIN_TEXT
$BR
Find the general solution to the differential equation
\[ y' + $alpha x^{\{$alpha -1\}} y = x^{$alpha}\]
$BR
Use the variable \( I = \int e^{x^{$alpha}}\, dx \) where it occurs in your answer.
$BR
\( y = {} \) \{ans_rule(60)\}

$PAR
END_TEXT

ANS(fun_cmp("(x*exp(x**($alpha)) - I)/($alpha*exp(x**($alpha)))", vars=>['x','I'], limits=>[[1,1.1],[1,10]]));

ENDDOCUMENT();




